{
  "name": "apnic-stats-charts",
  "version": "0.0.1",
  "description": "APNIC Stats Charts",
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "bump": "ncu --upgradeAll; export PKG=eslint-config-airbnb; npm info \"$PKG@latest\" peerDependencies --json | command sed 's/[\\{\\},]//g ; s/: /@/g' | xargs npm install --save-dev \"$PKG@latest\"; npm install -d;",
    "development": "cross-env NODE_ENV=development webpack-dev-server --config config.babel.js",
    "staging": "cross-env NODE_ENV=staging webpack --config config.babel.js",
    "production": "cross-env NODE_ENV=production webpack --config config.babel.js",
    "build": "npm run staging; npm run production"
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-airbnb": "^2.2.3",
    "bluebird": "^3.5.0",
    "bootstrap": "^3.3.7",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "npm-check-updates": "^2.11.1",
    "postcss-loader": "^2.0.5",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-bootstrap-datetimepicker": "0.0.22",
    "react-dom": "^15.5.4",
    "recharts": "^0.22.4",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "victory": "^0.19.1",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1"
  }
}
